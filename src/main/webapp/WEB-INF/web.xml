<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>FileToFileServlet</servlet-name>
		<servlet-class>com.allianz.re.testdataconvertor.FileToFileServlet</servlet-class>
	</servlet>
	
	
	
	<servlet>
		<servlet-name>DBToFileServlet</servlet-name>
		<servlet-class>com.allianz.re.testdataconvertor.DBToFileServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.allianz.re.testdataconvertor.LoginServlet</servlet-class>
	</servlet>
	
<servlet-mapping>
		<servlet-name>DBToFileServlet</servlet-name>
		<url-pattern>/dbTofile</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>FileToFileServlet</servlet-name>
		<url-pattern>/uploadFile</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	


</web-app>
